{
   "Version":"2012-10-17",
   "Id":"newsBucketPolicy",
   "Statement":[
      {
         "Sid":"PublicReadGetObject",
         "Effect":"Allow",
         "Principal":"*",
         "Action":[
            "s3:GetObject"
         ],
         "Resource":[
            "arn:aws:s3:::news4421-terraform-infra-static-pages/*"
         ]
      },
      {
         "Sid":"AllowBucketAccess",
         "Effect":"Allow",
         "Principal":"*",
         "Action":"s3:ListBucket",
         "Resource":"arn:aws:s3:::news4421-terraform-infra-static-pages"
      }
   ]
}